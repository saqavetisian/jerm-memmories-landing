{
  "name": "landing",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "node-dev": "nodemon",
    "node-build": "next build && tsc --project tsconfig.server.json",
    "node-build-cpu": "cross-env NEXT_CPU_THREADS=1 next build && tsc --project tsconfig.server.json",
    "node-build-cpu-limit": "cross-env NEXT_CPU_THREADS=1 NODE_OPTIONS=--max-old-space-size=2048 next build && tsc --project tsconfig.server.json",
    "node-start-limit": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=3072 node dist/server.js",
    "node-start": "cross-env NODE_ENV=production node dist/server.js",
    "node-build-disable": "cross-env NEXT_CPU_THREADS=1 NODE_OPTIONS=--no-warnings --disable-worker-threads next build && tsc --project tsconfig.server.json"
  },
  "dependencies": {
    "framer-motion": "^12.19.2",
    "lucide-react": "^0.525.0",
    "next": "15.2.3",
    "next-intl": "^4.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
